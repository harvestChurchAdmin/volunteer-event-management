    </div>
    <div class="modal-footer">
      <% if (typeof footer !== 'undefined') { %>
        <%- footer %>
      <% } else { %>
        <button class="btn btn-secondary" data-close="#<%= id %>">Cancel</button>
        <% if (typeof formId !== 'undefined' && formId) { %>
          <button class="btn btn-primary" form="<%= formId %>">Save</button>
        <% } %>
      <% } %>
    </div>
  </div>
</div>
